{
  "name": "@project/nuxt-webkit-module",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Nuxt 3 webkit contains components, plugins and composables for the website.",
  "exports": {
    ".": {
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --config vitest.config.ts --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.2.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.2.2",
    "@project/eslint-config-antfu": "workspace:*",
    "@project/tsconfig": "workspace:*",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/test-utils": "^2.3.0",
    "c8": "^7.13.0",
    "jsdom": "^21.1.0",
    "nuxt": "3.2.2",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.4",
    "vitest": "^0.28.5",
    "vue": "^3.2.47"
  }
}
