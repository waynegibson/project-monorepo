{
  "name": "@project/nuxt-webkit-module",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Nuxt 3 webkit contains components, plugins and composables for the website.",
  "exports": {
    ".": {
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --config vitest.config.ts --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@project/eslint-config-antfu": "workspace:*",
    "@project/tsconfig": "workspace:*",
    "@types/node": "^18.11.13",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.5",
    "@vue/test-utils": "^2.2.6",
    "c8": "^7.12.0",
    "jsdom": "^20.0.3",
    "nuxt": "3.0.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.5",
    "vitest": "^0.25.5",
    "vue": "^3.2.45"
  }
}
