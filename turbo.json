{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV"],
  "pipeline": {
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".output/**", ".cdk.out/**"]
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate:deploy": {
      "dependsOn": ["build"],
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}
